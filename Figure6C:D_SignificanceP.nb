Cell[CellGroupData[{

Cell["Definition of Graded Histogram", "Subsection",
 CellChangeTimes->{{3.777796891752336*^9, 3.7777968937121983`*^9}, {
  3.777875674721262*^9, 
  3.77787567720686*^9}},ExpressionUUID->"7368b82d-aaf6-4aff-a7a2-\
0733f8323813"],

Cell[BoxData[
 RowBox[{"Clear", "[", "GradedHistogram", "]"}]], "Input",
 CellChangeTimes->{{3.777868691935758*^9, 3.777868693968856*^9}},
 CellLabel->
  "In[151]:=",ExpressionUUID->"b4a06e28-addd-45fb-99b4-1aa9a39d6c37"],

Cell[BoxData[
 RowBox[{
  RowBox[{"GradedHistogram", "[", 
   RowBox[{"xL_", ",", "pL_", ",", 
    RowBox[{"nb_:", "9"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"pmax_:", ".99"}], ",", 
      RowBox[{"pmin_:", ".8"}]}], "}"}], ",", 
    RowBox[{"color_:", "Red"}], ",", "opts___"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "qdata", ",", "pos", ",", "qgrouped", ",", "pgrouped", ",", "psorted", 
      ",", "n", ",", "ydata", ",", "\[Delta]"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "xL", "]"}]}], ";", 
     RowBox[{"\[Delta]", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Differences", "[", 
         RowBox[{"MinMax", "[", "xL", "]"}], "]"}], "\[LeftDoubleBracket]", 
        "1", "\[RightDoubleBracket]"}], "/", 
       RowBox[{"(", 
        RowBox[{"nb", "-", "1"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"qdata", "=", 
      RowBox[{"Round", "[", 
       RowBox[{"xL", ",", "\[Delta]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{"Position", "[", 
          RowBox[{"qdata", ",", "#"}], "]"}], "]"}], "&"}], "/@", 
       RowBox[{"Round", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{
           RowBox[{"Min", "[", "qdata", "]"}], ",", 
           RowBox[{"Max", "[", "qdata", "]"}], ",", "\[Delta]"}], "]"}], ",", 
         "\[Delta]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"qgrouped", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Part", "[", 
         RowBox[{"qdata", ",", "#"}], "]"}], "&"}], "/@", "pos"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pgrouped", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Part", "[", 
         RowBox[{"pL", ",", "#"}], "]"}], "&"}], "/@", "pos"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"psorted", "=", 
      RowBox[{"Reverse", "/@", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"SortBy", "[", 
           RowBox[{"#", ",", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"#", "-", ".5"}], "]"}], "&"}]}], "]"}], "&"}], ")"}], "/@",
         "pgrouped"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ydata", "=", 
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"Length", "[", "#", "]"}], "]"}], "&"}], "/@", "psorted"}], 
       "n"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"pLogColor", "[", 
            RowBox[{
             RowBox[{"psorted", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], ",", "pmax",
              ",", "pmin", ",", "color"}], "]"}], ",", 
           RowBox[{"Rectangle", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"qgrouped", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "-", 
                RowBox[{"\[Delta]", "/", "2"}]}], ",", 
               RowBox[{
                RowBox[{"ydata", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "-", 
                RowBox[{"1", "/", "n"}]}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"qgrouped", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "+", 
                RowBox[{"\[Delta]", "/", "2"}]}], ",", 
               RowBox[{"ydata", "\[LeftDoubleBracket]", 
                RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], "}"}]}],
             "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"Length", "[", "pos", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", 
           RowBox[{"Length", "[", 
            RowBox[{
            "pos", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
            "]"}]}], "}"}]}], "]"}], ",", "opts"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
