Cell[BoxData[{
 RowBox[{
  RowBox[{"pLog", "[", "p_", "]"}], ":=", 
  RowBox[{"-", 
   RowBox[{"Log", "[", 
    RowBox[{"10.", ",", 
     RowBox[{".5", "-", 
      RowBox[{"Abs", "[", 
       RowBox[{"p", "-", ".5"}], "]"}]}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pLogColor", "[", 
   RowBox[{"0.", ",", "pmax_", ",", "pmin_", ",", 
    RowBox[{"color_:", "Red"}]}], "]"}], ":=", 
  "color"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pLogColor", "[", 
   RowBox[{"1.", ",", "pmax_", ",", "pmin_", ",", 
    RowBox[{"color_:", "Red"}]}], "]"}], ":=", 
  "color"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pLogColor", "[", 
   RowBox[{"Indeterminate", ",", "pmax_", ",", "pmin_", ",", 
    RowBox[{"color_:", "Red"}]}], "]"}], ":=", 
  "White"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pLogColor", "[", 
   RowBox[{"p_", ",", 
    RowBox[{"pmax_:", ".999"}], ",", 
    RowBox[{"pmin_:", ".8"}], ",", 
    RowBox[{"color_:", "Red"}]}], "]"}], ":=", 
  RowBox[{"Blend", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"GrayLevel", "[", ".8", "]"}], ",", "color"}], "}"}], ",", 
    FractionBox[
     RowBox[{
      RowBox[{"-", 
       RowBox[{"pLog", "[", "p", "]"}]}], "+", 
      RowBox[{"pLog", "[", "pmin", "]"}]}], 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"pLog", "[", "pmax", "]"}]}], "+", 
      RowBox[{"pLog", "[", "pmin", "]"}]}]]}], "]"}]}]}], "Input",
